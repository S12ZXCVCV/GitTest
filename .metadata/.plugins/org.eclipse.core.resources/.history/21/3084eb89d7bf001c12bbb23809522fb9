
public class Ex15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	int[] score= {79,88,91,33,100,55,95};
	
	int max=0 , min =0 , tmp=0;
	
	for(int i=0; i<7 ; i++) {
		if(score[i]>max) {
			max=score[i];}
	    if(score[i]<min) {
	    	min=score[i];}
		
	}
	
	System.out.println("max="+max);
	System.out.println("min="+min);
	}       

}
