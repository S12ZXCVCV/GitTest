


public class CallStackTest2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	System.out.println("main(String[] args이 시작도히었습니다.");	
	firstMethod();
	System.out.println("main(String[] args이 끝났습니다.");	
	
	
	
	}
	static void firstMethod() {
	System.out.println("firstMethod();이 시작도히었습니다.");	
	scecondMethod();
	System.out.println("firstMethod();이 끝났습니다.");	
	
	}
	static void scecondMethod() {
		System.out.println("scecondMethod();이 시작도히었습니다.");	
		System.out.println("scecondMethod();이 끝났습니다.");	
	}
}
