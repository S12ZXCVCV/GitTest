
public class Ex15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
	int[] score= new int[10];	
		
	for(int i=0; i<score.length; i++) {
	int n = (int)(Math.random()*10);
	System.out.print(n);
	}
	
	for(int j=0; j<score[i].length; j++)
	System.out.printf("%d의 개수: %d%n",j,2);
	}
}
